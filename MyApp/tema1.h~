#ifndef TEMA1_H
#define TEMA1_H

class tema1 {
private:
    int* data;  // Pointer pentru a arăta alocarea dinamică

public:
    // Constructor și Destructor
    tema1(int value);
    ~tema1();

    // Copy Constructor și Move Constructor
    tema1(const tema1& other);       // Copy constructor
    tema1(tema1&& other) noexcept;   // Move constructor

    // Getter pentru valoare
    int getData() const;

    // Setter pentru valoare
    void setData(int value);
};

#endif
